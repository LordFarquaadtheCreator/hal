{
  "name": "hal",
  "displayName": "hal",
  "description": "ai assisted debugger",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
          {
              "id": "hal",
              "title": "HAL",
              "icon": "hal.png"
          }
      ]
    },
    "views": {
    "hal": [
        {
          "id": "HALInstance",
          "name": "HAL"
        }
      ]
    },
    "commands": [
      {
        "command": "hal.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "openai": "^4.17.4",
    "punycode": "^2.3.1"
  }
}
