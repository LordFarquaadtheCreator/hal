{
  "name": "hal",
  "displayName": "HAL",
  "description": "AI assisted debugger",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:hal.openWebview",
    "onView:halView"
  ],
  "main": "extension.js",
  "icon": "halWhite.png",
  "contributes": {
    "commands": [
      {
        "command": "hal.openWebview",
        "title": "Open HAL Webview"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "hal",
          "title": "HAL",
          "icon": "halWhite.png"
        }
      ]
    },
    "views": {
      "hal": [
        {
          "id": "halView",
          "name": "HAL Interactive"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "eslint": "^8.52.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {}
}
